    "chalk": "^5.3.0",
    "cheerio": "^1.1.0",
    "commander": "^12.1.0",
    "electrodb": "^3.5.0",
    "hono": "^4.11.1",
    "isbot": "^5.1.32",
    "ora": "^8.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "7.11.0",
    "sst": "^3.17.25",
