{
  "name": "hotukdeals-discord-notifier",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "A Discord bot that monitors HotUKDeals and sends deal notifications to Discord channels",
  "scripts": {
    "build": "react-router build",
    "dev": "sst dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc -p tsconfig.app.json",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "feed-parser": "npx tsx scripts/run-feed-parser.ts",
    "manage-config": "npx tsx scripts/manage-config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllyMurray/hotukdeals-discord-notifier.git"
  },
  "keywords": [
    "hotukdeals",
    "discord",
    "notifier"
  ],
  "author": "ally-murray",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AllyMurray/hotukdeals-discord-notifier/issues"
  },
  "homepage": "https://github.com/AllyMurray/hotukdeals-discord-notifier#readme",
